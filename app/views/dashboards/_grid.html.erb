<div class="mt-4">
  <div class="row">
    <% items.each do |item| %>
      <div class="col-sm-6">
        <div class="card mt-3">
          <div class="card-body">
            <h5 class="card-title"><%= item.title %></h5>
            <p class="card-text"><%= item.description %></p>
            <a href=<%=item.link%> class="btn btn-primary">Manage</a>
          </div>
        </div>
      </div>
    <% end%>
  </div>
</div>
