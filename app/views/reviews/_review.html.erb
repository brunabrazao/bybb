      <h3><%= review_question[1] %></h3>
      <p class="mt-2"><%= review.answer_two %></p>
    </div>
    <div  class="mt-5">
      <h3><%= review_question[2] %></h3>
      <p class="mt-2"><%= review.answer_three %></p>
    </div>
    <% if review_question[3].present? %>
