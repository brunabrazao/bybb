<% review_question = review.reviews_cycle.questions.compact_blank.values %>
<div id="<%= dom_id review %>" class="mt-5">
  <div class="bg-light p-5 rounded">
    <div class="mt-3">
      <h3><%= review_question[0] %></h3>
      <p class="mt-2"><%= review.answer_one %></p>
    </div>
