<div id="<%= dom_id reviews_cycle %>">
  <p>
    <strong>Name:</strong>
    <%= reviews_cycle.name %>
  </p>
  <p>
    <strong>Organisation:</strong>
    <%= reviews_cycle.organisation.title%>
  </p>
  <p>
    <strong>Nominated users count:</strong>
    <%= reviews_cycle.selected_users.count%>
  </p>
  <p>
    <strong>Users</strong>
    <% reviews_cycle.users.map do |user| %>
      <li><%= user.email%></li>
    <% end %>
  </p>
</div>
